{
  "compilerOptions": {
    "target": "ES2020",
    "module": "CommonJS",
    "lib": ["esnext"],
    "jsx": "preserve",
    "strict": true,
    "pretty": true,
    "sourceMap": false,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "moduleResolution": "node",
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "resolveJsonModule": true,
    "outDir": "../main_dist",
    "paths": {
    }
  },
  "include": [
    "."
  ],
}
